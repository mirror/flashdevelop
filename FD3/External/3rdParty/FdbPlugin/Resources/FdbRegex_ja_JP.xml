<?xml version="1.0"?>
<FdbRegex xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Locate>ja_JP</Locate>
  <MsgList>
    <MsgPattern>
      <Name>UnknownCommandFormat</Name>
      <Pattern><![CDATA[不明なコマンド [{0}] が見つかりました。このコマンドは無視されます。]]></Pattern>
    </MsgPattern>
  </MsgList>
  <RegexList>
    <RegexPattern>
      <Name>RegexNameValue</Name>
      <Pattern><![CDATA[(?<name>.*).*?( = )(?<value>.*)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexObject</Name>
      <Pattern><![CDATA[.*\[Object\s\d*, class='.*'\]]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexSetBreakPoint</Name>
      <Pattern><![CDATA[ブレークポイント (?<breakpoint>.*).*?: ファイル (?<filename>.*).*?、行番号 (?<line>.*)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexPrintObject</Name>
      <Pattern><![CDATA[.*?(\s=\s)(?<name>.*).*?(\s=\s).*]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexPrintVal</Name>
      <Pattern><![CDATA[.*?(\s=\s)(?<value>.*)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexPrintValUnknown</Name>
      <Pattern><![CDATA[変数\s\S*\sが不明です]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexUnloadSWF</Name>
      <Pattern><![CDATA[^?\[UnloadSWF\]]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexException</Name>
      <Pattern><![CDATA[^?\[フォルト\]]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexStopBreakPoint</Name>
      <Pattern><![CDATA[理由 : ブレークポイント (?<breakpointnum>\d+).*?、.*?:\d+]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexCf</Name>
      <Pattern><![CDATA[(?<filename>.*).*?#(?<num>\d+).*:(?<line>\d+)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexShowFiles</Name>
      <Pattern><![CDATA[(?<num>\d+).*?\D(?<filefullpath>.*).*?,.*]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexInfoFunctionsFileInfo</Name>
      <Pattern><![CDATA[(?<filename>.*).*?#(?<num>\d+).*?( 内の関数)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexInfoFunctionsFunctionInfo</Name>
      <Pattern><![CDATA[\s(?<function>.*).*?\s(?<line>\d+)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexDisply</Name>
      <Pattern><![CDATA[\d*?(: ')(?<command1>.*).*?(' = )(?<command2>.*)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexUnknownCommand</Name>
      <Pattern><![CDATA[不明なコマンド \[(?<command>.*).*?\] が見つかりました。このコマンドは無視されます。]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexStack</Name>
      <Pattern><![CDATA[^#(?<framenum>\d+).*?\s.*]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexFrame</Name>
      <Pattern><![CDATA[.*?( 場所 :  )(?<filename>.*).*?#(?<srcfilenum>\d+).*?:(?<line>\d+)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexTrace</Name>
      <Pattern><![CDATA[^?\[trace\]]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexNotRespond</Name>
      <Pattern><![CDATA[Player がコマンドに対して予期された応答をしなかったため、コマンド処理は中止されました。]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexTerminated</Name>
      <Pattern><![CDATA[Player セッションが終了しました]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexPause</Name>
      <Pattern><![CDATA[実行が中止されました .*?(アドレス :) 実行が中止されました .*?(\s\().*?(\)$)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexLoadSWF</Name>
      <Pattern><![CDATA[^\[SWF\].*]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexCheckStart</Name>
      <Pattern><![CDATA[^\[SWF\].*?(バイト \(解凍後\))]]></Pattern>
    </RegexPattern>  
    <RegexPattern>
      <Name>RegexLoadSWForFrame</Name>
      <Pattern><![CDATA[SWF ファイルまたはフレームから追加の ActionScript コードがロードされました。]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexAskCondition</Name>
      <Pattern><![CDATA[式に代入要素が含まれています。続行しますか? \(y または n\)]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexClearCondition</Name>
      <Pattern><![CDATA[ブレークポイント (\d+) が解除されました]]></Pattern>
    </RegexPattern>
    <RegexPattern>
      <Name>RegexFinishError</Name>
      <Pattern><![CDATA[一番外側のフレームでは、'finish' は無意味です]]></Pattern>
    </RegexPattern>
  </RegexList>
</FdbRegex> 
